<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js" async></script>

<!-- Template Main JS File -->
<script src="~/assets/js/main.js" async></script>

@{
    var modalData = TempData.Get<ModalData>("modal");

    if (modalData.IsNotNull())
    {
        <script type='text/javascript' async>
        $(document).ready(function () {
            $('#@modalData.Type').modal('show');
        });
        </script>
    }
}