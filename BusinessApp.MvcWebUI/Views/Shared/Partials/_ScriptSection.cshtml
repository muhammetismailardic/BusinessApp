@using BusinessApp.CarpetWash.MvcWebUI.Shared

<!-- Vendor JS Files -->
<script src="~/assets/vendor/jquery/jquery.min.js" async></script>
<script src="~/assets/vendor/bootstrap/js/bootstrap.bundle.min.js" async></script>
<script src="~/assets/vendor/jquery.easing/jquery.easing.min.js" async></script>
@*<script src="~/assets/vendor/php-email-form/validate.js"></script>*@
<script src="~/assets/vendor/jquery-sticky/jquery.sticky.js" async></script>
<script src="~/assets/vendor/isotope-layout/isotope.pkgd.min.js" async></script>
<script src="~/assets/vendor/venobox/venobox.min.js" async></script>
<script src="~/assets/vendor/waypoints/jquery.waypoints.min.js" async></script>
<script src="~/assets/vendor/owl.carousel/owl.carousel.min.js" async></script>
<script src="~/assets/vendor/aos/aos.js"></script>

<script src="~/lib/jquery/jquery.min.js"></script>
